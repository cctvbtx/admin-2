{
    "name": "yii2tech/admin",
    "description": "Admin pack (actions, widgets, etc) for Yii2",
    "keywords": ["yii2", "admin", "administration", "crud"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yii2tech/admin/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/yii2tech/admin/wiki",
        "source": "https://github.com/yii2tech/admin"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/yii2": "2.1.x-dev"
    },
    "require-dev": {
        "yiisoft/yii2-bootstrap": "2.2.x-dev"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "you need this package, if you wish to use provided widgets"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"yii2tech\\admin\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"yii2tech\\tests\\unit\\admin\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
